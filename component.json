{
    "title": "Hook Component",
    "description": "Upsert Hook logs and consolidated hooks",
    "docsUrl": "",
    "buildType": "docker",
    "envVars": {
        "API_KEY": {
            "required": true,
            "description": "Kindly enter the API Key for the MongoDB Data APIs to be functional"
        }
    },
    "credentials": {
        "mongo_credential": {
            "apiKey": "{{API_KEY}}"
        }
    },
    "triggers": {
        "startFlow": {
            "main": "./lib/triggers/startFlow.js",
            "type": "polling",
            "title": "Start Flow",
            "fields": {}
        }
    },
    "actions": {
        "getUsers": {
            "main": "./lib/actions/getUsers.js",
            "title": "Upsert Hook",
            "metadata": {
                "in": "./lib/schemas/getUsers.in.json"
            }
        }
    }
}